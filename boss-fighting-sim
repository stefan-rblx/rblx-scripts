local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/wally-rblx/uwuware-ui/main/main.lua", true))()
local window = library:CreateWindow("Farming")
local window2 = library:CreateWindow("Teleports")

--teleports

window2:AddButton({text = 'Healing Well', callback = function()
 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-105, -33, 73)
end})

window2:AddButton({text = 'Islands', callback = function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(18, 48, 6140)
end})

--farming
window:AddToggle({text = "Auto Swing", callback = function(bool)
getgenv().autohit = false
    getgenv().autohit = bool
    if bool then 
        autohit()
    end
end})

window:AddToggle({text = "Auto Sell", callback = function(bool)
    getgenv().sell = false
    getgenv().sell = bool
    if bool then 
        autosell()
    end
end})

window:AddDivider()

window:AddToggle({text = "Noob Boss", callback = function(bool)
    getgenv().nb = false
        getgenv().nb = bool
        if bool then 
            while wait() do 
                if not getgenv().nb then break end
            for i,v in pairs(game:GetService("Workspace").Arenas.NoobArena.Boss.Noob:GetChildren()) do 
                if v:IsA("Part") and v.Name == "Head" then
                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                    else 
                    print("ok innit")
                end
            end
        end
    end
end})

